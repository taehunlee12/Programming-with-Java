public class NestedLoop
{
    public static void calculate() {
        for(int i = 1; i <= 9; i++){
            for(int j = 1; j <= 9; j++){
                int result = 0;
                result = i * j;
                System.out.print(i + "*" + j + "=" + result + "  ");
                
            }
            System.out.println();
        }
    }
    public static void main(String[] args) { //static 메쏘드 
        calculate();

    }
}
